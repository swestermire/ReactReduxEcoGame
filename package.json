    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "jquery": "^2.2.1",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^0.14.7",
    "redux-devtools": "^3.4.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
 },
  "dependencies": {
    "babel-preset-stage-1": "^6.1.18",
    "bootstrap": "^3.3.7",
